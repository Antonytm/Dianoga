<!--
	Configures Dianoga to optimize JPEG images
	By default this uses the MozJpeg library, which results in good file size reduction for the web.
-->
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<pipelines>
			<dianogaGetSupportedFormats>
				<!-- if adding new processors make sure they have a unique desc attribute -->
				<processor type="Dianoga.NextGenFormats.Pipelines.DianogaGetSupportedFormats.CheckSupport, Dianoga" desc="avif">
					<Extensions>avif</Extensions>
				</processor>
			</dianogaGetSupportedFormats>
		</pipelines>
	</sitecore>
</configuration>